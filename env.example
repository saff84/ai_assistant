# ------------------------------------------------------------------
# Copy this file to `.env` and adjust values before running locally.
# Never commit the populated `.env` file to source control.
# ------------------------------------------------------------------

# Runtime
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=mysql://assistant:assistant_password@localhost:3306/ai_knowledge_db
MYSQL_ROOT_PASSWORD=root_password
MYSQL_DATABASE=ai_knowledge_db
MYSQL_USER=assistant
MYSQL_PASSWORD=assistant_password

# Authentication & security
JWT_SECRET=change-me
DEV_MODE=true
DEV_ADMIN_EMAIL=admin@admin.local
DEV_ADMIN_NAME=Admin User
OWNER_OPEN_ID=
ADMIN_EMAIL=admin@admin.local
ADMIN_PASSWORD=admin123
ADMIN_NAME=Administrator

# OAuth / Manus portal (optional)
OAUTH_SERVER_URL=
VITE_OAUTH_PORTAL_URL=
VITE_APP_ID=

# Frontend branding
VITE_APP_TITLE=AI Knowledge Assistant
VITE_APP_LOGO=/logo.svg

# Forge API (client + server)
VITE_FRONTEND_FORGE_API_KEY=
VITE_FRONTEND_FORGE_API_URL=https://forge.butterfly-effect.dev
BUILT_IN_FORGE_API_URL=
BUILT_IN_FORGE_API_KEY=

# RAG / LLM configuration
OLLAMA_URL=http://localhost:11434
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=qwen2.5:1.5b-instruct
LLM_MODEL_GENERATION=qwen2.5:1.5b-instruct
EMBEDDING_MODEL=bge-m3
OLLAMA_EMBEDDING_MODEL=bge-m3
RERANKER_URL=
RERANK_MODEL=bge-reranker-v2-m3
RERANK_ENABLED=true
RAG_RERANK_ENABLED=true
LLM_TEMPERATURE=0.2
LLM_TOP_P=0.9
LLM_REPEAT_PENALTY=1.1
RAG_CONFIG_PATH=./config/rag.json
RAG_SYSTEM_PROMPT_PATH=./prompts/system.sanext.txt

# Integrations
BITRIX24_WEBHOOK_URL=

# Observability / misc
WEAVIATE_URL=http://localhost:8080

